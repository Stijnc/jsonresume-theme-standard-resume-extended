{{#if resume.volunteer.length}}
	{{#each resume.volunteer}}
		<section class="position">
			{{#if @first}}
				<h2>Community</h2>
			{{/if}}
			<h3>
				{{#if url}}
					<a target="_blank" rel="noopener" href="{{url}}">{{organization}}</a>
				{{else}}
					{{organization}}
				{{/if}}
			</h3>
			<h4>
				{{#if position}}
					{{position}}
				{{/if}}

				{{> date-range}}
			</h4>

			{{#if summary}}
				<p>{{summary}}</p>
			{{/if}}
			{{#if highlights.length}}
				<ul class="highlights">
					{{#each highlights}}
					<li>{{.}}</li>
					{{/each}}
				</ul>
			{{/if}}
		</section>
	{{/each}}
{{/if}}


